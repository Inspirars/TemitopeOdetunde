<%- include("./layout/header.ejs") %>
    <main class="bg-white overflow-x-hidden">
      <nav class="flex bg-grey-100 justify-between items-center py-4 px-4 md:px-10 md:pt-8 lg:px-20 font-inter  fixed w-full overflow-hidden " id="nav">
        <div class="lg:flex items-center gap-[72px]">
            <a href="/"><img src="/images/logo.svg" alt="Temitope svg"></a>
            <ul class=" hidden lg:flex list-none lg:gap-11 text-base text-black-200 font-normal">
              <a href="">
                <li>About</li>
              </a>
              <a href="/books">
                <li>Book</li>
              </a>
              <a href="">
                <li>Shop</li>
              </a>
              <a href="">
                <li>AIM</li>
              </a>
              <a href="">
                <li>BLOG</li>
              </a>
            </ul>
        </div>
        <img src="/images/hamburger.png" alt="" class="lg:hidden">
        <div class="hidden lg:flex" >
            <button class="text-lg md:px-5 md:py-4 rounded font-semibold bg-purple text-white border border-purple hover:bg-grey-100 hover:text-purple transition-all duration-700">
                Join Community
            </button>
        </div>
      </nav>
      <section class=" bg-grey-100 px-5 pt-24 pb-11 lg:pt-44 lg:py-[clamp(85px,15.62vh,174px)] lg:pl-[clamp(85px,8.125vw,117px)] font-satoshi">
        <h1 class="text-3xl md:text-5xl lg:text-[clamp(85px,8vw,120px)] leading-none font-extrabold">
          EMPOWERING CAREERS, ENRICHING SOULS.
        </h1>
        <p class="mt-6 lg:mt-[clamp(20px,5.54vh,48px)] lg:text-3xl font-normal w md:w-[75%] text-balance">
          Experience the power of faith-driven success, empowering individuals to excel in their careers while nurturing their souls.
        </p>
      </section>
      <section class="bg-purple bg-bio bg-no-repeat flex justify-center relative font-satoshi">
        <img src="/images/curly.png" alt="">
        <div class="absolute bottom-0  md:bottom-28 md:left-12 bg-white rounded-xl p-3 md:p-5 lg:rounded-3xl flex flex-col gap-2 md:gap-5 items-start text-grey-200 w-[90%] max-w-[clamp(650px,62vw,897px)]">
          <h4>
            Who am I?
          </h4>
          <h1 class="lg:text-6xl font-bold text-black-200">
            Temitope Odetunde
          </h1>
          <p class="text-xs lg:text-xl text-pretty">
            Your guide to navigating the intersection of professional success and spiritual fulfillment. As a leading voice in pharmacy consultancy and faith-based personal development, Temitope blends her extensive expertise in the pharmaceutical industry with a deep-rooted commitment to Christian values. With Temitope, you'll discover a path to excellence in your career while staying true to your faith, driven by a passion for empowering individuals to achieve greatness in all aspects of their lives.
          </p>
          <button class="bg-black-200 border border-black-200 text-white hover:bg-white hover:text-black-200 transition-all duration-700 py-2 px-3 md:py-4 md:px-5 rounded text-xs md:text-lg">
            Read More
          </button>
        </div>
      </section>
      <section class="bg-[#000] px-5 md:px-14 lg:px-28 py-12 md:py-24 font-satoshi">
        <p class="text-[#E4E7EC] text-xl md:text-[clamp(20px,2.222vw,32px)] md:leading-9 lg:w-[75%]">
          As a mentor in faith-based personal development, I support individuals in aligning their professional goals with their spiritual values. Together, we embark on a journey towards excellence, driven by passion, expertise, and unwavering faith.
        </p>
        <div class="grid grid-cols-1 gap-6 md:grid-cols-2 md:gap-16 mt-12 lg:mt-16 text-[#D0D5DD] text-lg md:text-2xl">
          <p>Pharmacy Consultant and Expert</p>
          <p>Author and Speaker</p>
          <p>Leader in Medication Management Consulting</p>
          <p>Innovator in Regulatory Compliance Solutions</p>
          <p>Faith-Based Personal Development Mentor</p>
          <p>Advocate for Care Home Policies and Procedures</p>
        </div>
  
      </section>
      <section class="bg-[#1D2739] px-5 md:px-14 lg:px-24 py-12 lg:py-28 font-satoshi">
        <div class="flex flex-col gap-5 lg:flex-row lg:items-center lg:justify-between">
          <div class="flex flex-col items-start md:flex-row md:items-center text-white gap-5">
            <img src="/images/medal.png" alt="">
            <h1 class="text-4xl lg:text-7xl font-bold ">Ambassadors <br>
              <span class="font-normal">in the marketplace</span>
            </h1>
          </div>
          <div class="flex items-center  gap-4">
            <button class="text-white bg-purple rounded px-4 py-3 md:py-4 md:px-5"> 
              Visit AIM Page
            </button>
            <button class="text-purple border border-purple rounded px-4 py-3 md:py-4 md:px-5">
              Visit youtube
            </button>
          </div>
        </div>
        <div class="mt-16 lg:mt-[90px] grid grid-cols-1 md:grid-cols-2 lg:grid-cols-5 gap-6">
          <% constants.forEach((item,index) => {%>
            <%if(index == 1 || index == 2){%>
              <div class=" lg:col-span-3 bg-[#D0D5DD] rounded-md lg:rounded-3xl p-5 lg:p-14 ">
                <img src="/images/Verified.png" alt="verify icon" class="pb-5">
                <p class="text-xl md:text-2xl lg:text-3xl"><%=item %></p>
              </div>
              <%return%>
            <%}%>
            <div class="bg-biogrey bg-no-repeat lg:col-span-2 bg-[#E4E7EC] rounded-lg lg:rounded-3xl p-5 lg:p-14 " id="veri">
              <img src="/images/Verified.png" alt="verify icon" class="pb-5">
              <p class="text-xl md:text-2xl lg:text-3xl"><%=item %></p>
            </div>
          <%})%>
        </div>
      </section>
  
      <section class="py-20 px-5 lg:py-[clamp(85px,15.62vh,144px)] bg-[#F9F4FF] lg:pl-[clamp(85px,8.125vw,117px)] font-satoshi">
        <h1 class="text-w[#000] text-3xl leading-[50px] font-normal lg:text-[64px] lg:leading-[90px] mb-7 md:mb-14">
          What people say <br> about Temitope
        </h1>
        <div class="swiper">
          <div class="swiper-wrapper">
            <% comments.forEach(commentus => {%>
              <div class="swiper-slide bg-white px-10 py-[61px] rounded-[30px] text-[#475367] cursor-pointer slidey " >
                <h3 class="mb-5 text-lg md:text-2xl font-normal"><%= commentus.comment%></h3>
                <h2 class="font-bold text-lg md:text-2xl" ><%= commentus.name %></h2>
                <p class="text-lg md:text-2xl font-normal"><%= commentus.location%></p>
              </div>
            <%})%>
          </div>
        </div>
      </section>
  
      <section class="bg-white px-5 md:px-14 lg:px-24 py-12 lg:py-28 font-satoshi">
        <div class="flex flex-col gap-5 items-start lg:items-center lg:flex-row lg:gap-[62px] lg:pl-[150px]">
          <h4 class="text-[#4B587C] lg:w-[49%] text-xl md:text-[clamp(20px,2.222vw,32px)]">
            Whether you're seeking professional guidance, personal growth insights, or spiritual enrichment, our library has something for everyone.
          </h4>
          <button class="bg-black-200 border border-black-200 text-white hover:bg-white hover:text-black-200 transition-all duration-700 py-2 px-3 md:py-4 md:px-5 rounded text-xs md:text-lg">
            Visit Book Store
          </button>
        </div>
        <div>
          <div class="swiper2 mobSwiper lg:hidden mt-10">
            <div class="swiper-wrapper">
              <% bookCovers.forEach(bookcover => {%>
                <div class="swiper-slide">
                  <img src=<%= bookcover.imgSrc %> alt="bookcover">
                </div>
              <%})%>
            </div>
          </div>
          <div class="hidden lg:flex gap-5 mt-28">
            <% bookCovers.forEach(bookcover => {%>
              <div>
                <img class="hover:scale-90 transition-transform duration-700 cursor-pointer" src=<%= bookcover.imgSrc %> alt="bookcover">
              </div>
            <%})%>
          </div>
        </div>
        <div>
          <img src="/images/bookCoverImgs/bottomWood.png" alt="bottom Wood">
        </div>
      </section>
      <section class="bg-[#000] px-5 md:px-14 lg:px-[102px] py-12 lg:py-20 font-satoshi">
        <div class="flex flex-col gap-4 text-white items-start lg:flex-row lg:items-center lg:justify-between">
          <h3 class="text-2xl md:text-3xl lg:text-[40px]">Get educated from my blog</h3>
          <button class="bg-purple rounded px-4 py-3 md:py-4 md:px-5"> 
            Visit Blog
          </button>
        </div>
        <div class="blogswiper mt-10 lg:mt-28">
          <div class="swiper-wrapper ">
            <% blog.forEach(cover => {%>
              <div class="swiper-slide bg-white p-4 rounded-xl ">
                <img class="w-full" src=<%= cover.img %> alt="cover">
                <div>
                  <p class="bg-[#F2E5FF] rounded-md mt-3 md:mt-6 py-1 px-2 w-fit text-purple text-xs md:text-sm font-medium"><%=cover.tag %></p> 
                  <h2 class="mt-2 md:mt-4 text-lg font-bold lg:text-2xl"><%=cover.heading %></h2>
                  <div class="flex items-center gap-3 lg:gap-5 mt-3">
                    <img src=<%=cover.authorDp %> alt="">
                    <p class="text-xs md:text-base"><%=cover.authorName %></p>
                    <p class="text-xs md:text-base"><%=cover.date %></p>
                  </div>
                </div>
              </div>
            <%})%>
          </div>
        </div>
      </div>
      </section>
      <section class="bg-white px-5 md:px-14 lg:px-[102px] py-12 lg:py-20 font-satoshi">
        <div class="flex flex-col items-center justify-center gap-3">
          <h1 class="text-[#000] text-2xl md:text-3xl lg:text-[40px] font-medium lg:leading-[56px]">Get our merch from Esty</h1>
          <button class="bg-purple text-white rounded px-4 py-3 md:py-4 md:px-5"> 
            Shop for merch
          </button>
        </div>
        <div class="merchswiper mt-10 lg:mt-28">
          <div class="swiper-wrapper ">
            <% merch.forEach(merchs => {%>
              <div class="swiper-slide bg-[#F9F4FF]  p-4 rounded-xl ">
                <img class="w-full" src=<%= merchs.img %> alt="cover">
                <div class="text-[#181A2A]">
                  <p class="rounded-md mt-3 md:mt-5 text-lg lg:text-2xl font-normal"><%=merchs.item %></p> 
                  <h2 class="mt-2 md:mt-3 text-lg font-bold lg:text-2xl"><%=merchs.price %></h2>
                </div>
              </div>
            <%})%>
          </div>
        </div>
      </section>
      <section class="bg-[#FFF3E1] px-5 md:px-14 lg:px-24 py-12 lg:py-28 font-satoshi grid grid-cols-1 lg:grid-cols-5 gap-5 mb-14 lg:mb-28">
        <div class="bg-[#000] lg:col-start-1 lg:col-end-3 bg-bio bg-50 md:bg-25 lg:bg-100 bg-bottome rounded-xl bg-no-repeat text-[#E8E8EA] py-10 lg:pt-[76px] px-10 flex flex-col gap-8 lg:gap-12">
          <h2 class="text-2xl lg:text-[32px] font-bold">
            Contact me via
          </h2>
          <p class="flex items-center gap-4">
            <img src="/images/Letter.svg" alt="letter"> hello@temitope.com
          </p>
          <p class="flex items-center gap-4">
            <img src="/images/Phone.svg" alt="Phone"> +234 (81) 47511 481
          </p>
        </div>
        <div class="bg-[#FAAF40] bg-bio bg-tope bg-100 bg-no-repeat lg:col-start-3 flex justify-center lg:col-end-6 max-h-[500px] rounded-xl" >
          <img src="/images/curly.png" alt="" class="max-h-[500px]" >
        </div>
      </section>
    </main>
<%- include("./layout/footer.ejs")%>
